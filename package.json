{
  "name": "remark-bilibili-autolink",
  "version": "0.0.1",
  "description": "Remark plugin to autolink to Bilibili videos and articles",
  "keywords": [
    "remark",
    "bilibili"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "es/index.js",
  "browser": "dist/index.js",
  "repository": "https://github.com/dumboz/remark-bilibili-link.git",
  "author": "dumboz <heydumboz@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "tslint:fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage --collectCoverage",
    "typegen": "tsc --declaration --emitDeclarationOnly --declarationDir lib",
    "clean": "rimraf lib es dist",
    "build": "npm run clean && rollup -c && npm run typegen"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/unist": "^2.0.3",
    "@types/vfile": "^3.0.2",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "remark": "^10.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "unist-util-is": "^2.1.2",
    "unist-util-visit": "^1.4.0"
  }
}
